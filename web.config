<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\olx-be-api.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
      <rewrite>
        <rules>
          <rule name="Admin Route" stopProcessing="true">
            <match url="^admin/?$" />
            <action type="Rewrite" url="/admin/login.html" />
          </rule>
        </rules>
      </rewrite>
    </system.webServer>
  </location>
</configuration>